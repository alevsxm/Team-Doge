  <script>ZiggeoApi.token = "1a96227f1aca8f46d85f7a270b8e8266";</script>

</head>
<body>
<% if current_user %>
<header>
  <nav>
    <ul>
      <li>Welcome, <%= current_user.resovision.first_name %></li>
      <li><%= link_to 'Log Out', logout_path %></li>
      <li><%= link_to 'Home', user_path(current_user) %></li>
    </ul>
  </nav>
</header>
<% end %>

<%= yield %>

</body>
</html>
